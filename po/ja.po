#: ../ganglist/main.py:27
#, python-format
msgid "graph width (default: %d)"
msgstr "グラフの幅 (既定値: %d)"

#: ../ganglist/main.py:31
#, python-format
msgid "graph height (default: %d)"
msgstr "グラフの高さ (既定値: %d)"

